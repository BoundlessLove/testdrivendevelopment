<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" metadata-complete="false" version="3.0">
  <display-name>jspHello</display-name>
  <welcome-file-list>
    <!--<welcome-file>hello.jsp</welcome-file>-->
      <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>main.java.testdrivendevelopment.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/src/main/testdrivendevelopment/MyServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DisplayMonth</servlet-name>
    <servlet-class>main.java.testdrivendevelopment.DisplayMonth</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayMonth</servlet-name>
    <url-pattern>/src/main/testdrivendevelopment/DisplayMonth</url-pattern>
  </servlet-mapping>
    <servlet>
    <description></description>
    <display-name>Logout</display-name>
    <servlet-name>Logout</servlet-name>
    <servlet-class>main.java.testdrivendevelopment.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/src/main/testdrivendevelopment/Logout</url-pattern>
  </servlet-mapping>
</web-app>